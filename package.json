{
  "name": "carambar-project-front",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "homepage": "https://tibononox.github.io/carambar-project-front/",
    "dev": "vite",
    "build": "vite build && mkdir -p ./dist/src && cp -r ./src/assets ./dist/src",
    "preview": "vite preview",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist",
    "lint": "echo '----- Checking Frontend...' && prettier --debug-check . && eslint --ext .jsx . && echo '----- Done!'",
    "fix": "prettier --write . && eslint --fix --ext .jsx ."
  },
  "dependencies": {
    "axios": "^1.13.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "vite": "^7.3.1"
  }
}
